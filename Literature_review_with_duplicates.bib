@Article{Assor2024,
  author     = {Assor, Ambre and Prouzeau, Arnaud and Dragicevic, Pierre and Hachet, Martin},
  journal    = {ACM J. Comput. Sustain. Soc.},
  title      = {Augmented Reality Waste Accumulation Visualizations},
  year       = {2024},
  month      = {may},
  number     = {2},
  volume     = {2},
  abstract   = {The negative impact humans have on the environment is partly caused by thoughtless consumption leading to unnecessary waste. A likely contributing factor is the relative invisibility of waste: waste produced by individuals is either out of their sight or quickly taken away. Nevertheless, waste disposal systems sometimes break down, creating natural information displays of waste production that can have educational value. We take inspiration from such natural displays and introduce a class of situated visualizations we call augmented reality waste accumulation visualizations, or ARwavs, which are literal representations of waste data embedded in users’ familiar environment. We implemented examples of ARwavs and demonstrated them in feedback sessions with experts in pro-environmental behavior, and during a large tech exhibition event. We discuss general design considerations for ARwavs. Finally, we conducted a study with 20 participants suggesting that ARwavs yield stronger emotional responses than nonimmersive waste accumulation visualizations and plain numbers.},
  address    = {New York, NY, USA},
  articleno  = {11},
  doi        = {10.1145/3636970},
  issue_date = {June 2024},
  keywords   = {Augmented reality, eco-feedback, embedded visualizations, concrete scales},
  numpages   = {29},
  publisher  = {Association for Computing Machinery},
  url        = {https://doi.org/10.1145/3636970},
}

