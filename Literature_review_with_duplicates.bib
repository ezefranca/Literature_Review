@InProceedings{Breunig2021,
  author    = {Breunig, Julien and Calder\'{o}n Arias, Paola Stephania and Mudavanhu, Phillip and Syal, Abhay and Schulte, Britta and Hornecker, Eva},
  booktitle = {Proceedings of Mensch Und Computer 2021},
  title     = {Carpet Tiles: Interactive and Modular Carpets to support International Students in Shared Dorms to Feel at Home},
  year      = {2021},
  address   = {New York, NY, USA},
  pages     = {140–144},
  publisher = {Association for Computing Machinery},
  series    = {MuC '21},
  abstract  = {Moving to another country to study can be daunting and is connected to practical and emotional challenges. Here we present the results of an exploratory study on how smart textiles could be used to support international students in student housing to feel at home. We undertook seven situated interviews. Based on three key findings - the importance of social connections, cultural elements (food and carpets) and the limited duration of stay – the concept of a modular carpet with different interactive elements was developed. The carpet offers a social space, where students can spend time with friends. Its modularity allows personalization to the available space and room layout. With a proof of concept prototype we aim to open up a design space for tangible (textile) interventions.},
  doi       = {10.1145/3473856.3473995},
  isbn      = {9781450386456},
  keywords  = {flat share, international students, smart carpets, smart textiles, tangible interaction},
  location  = {Ingolstadt, Germany},
  numpages  = {5},
  url       = {https://doi.org/10.1145/3473856.3473995},
}

@Article{Rossitto2021,
  author     = {Rossitto, Chiara and Korsgaard, Henrik and Lampinen, Airi and B\o{}dker, Susanne},
  journal    = {Proc. ACM Hum.-Comput. Interact.},
  title      = {Efficiency and Care in Community-led Initiatives},
  year       = {2021},
  month      = {oct},
  number     = {CSCW2},
  volume     = {5},
  abstract   = {This paper illustrates the multifaceted aspects of caring practices, and the ways they are entangled with the organizing of community-driven initiatives. Highlighting the situated inter-dependencies between concerns for care and efficiency, and considering caring practices as essential to the practical work that makes communities work, we reflect on how caring and efficiency rationalities frame the use, and scope the design of digital technologies. Drawing on two cases, the analysis shows the ways in which digital technologies oftentimes overshadow communities' key concerns for care, and how attempts to design for community settings can result in anti-designs, that is sociotechnical configurations that can disrupt caring practices. The contribution of the paper is twofold: first, an analysis of the different configurations of caring and efficiency and, second, a focus on care in the design and appropriation of technologies into this space.},
  address    = {New York, NY, USA},
  articleno  = {467},
  doi        = {10.1145/3479611},
  issue_date = {October 2021},
