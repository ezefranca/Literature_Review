@InProceedings{Li2022,
  author    = {Li, Yueran and Xie, Xin},
  booktitle = {Proceedings of the 6th International Conference on Education and Multimedia Technology},
  title     = {An Analysis of the Construction of Online Design Workshop Education Model in the Post-Epidemic Era},
  year      = {2022},
  address   = {New York, NY, USA},
  pages     = {226–231},
  publisher = {Association for Computing Machinery},
  series    = {ICEMT '22},
  abstract  = {In the post-epidemic era, online teaching has become a trend, and "workshop" as an important teaching practice method for design disciplines has also been moved online. The teaching format of the workshop is usually practice-oriented, and many factors such as geographical distance and time difference have a great influence on the development of online teaching activities. Taking the teaching practice of the POLAR International Workshop as an example, this study obtained detailed experience and suggestions on the practical process and activities of the workshop through one-to-one semi-structured interviews with the participants in the workshop. Card sorting was used to analyze the users' information data, from the results, an online design workshop construction model was extracted. This qualitative study focused on how to create an online design workshop with a better experience. The online design workshop construction model in the post-epidemic era should start with three aspects, which were workshop agenda advancement, design work development and teamwork communication, focusing on the experience and feelings of participants. This study provided a structured empirical reference and guidance framework for the development of online workshop teaching activities in design disciplines in the future.},
  doi       = {10.1145/3551708.3551765},
  isbn      = {9781450396455},
  keywords  = {Design Education, Online Collaborate, Online Education, Workshop},
  location  = {Guangzhou, China},
  numpages  = {6},
  url       = {https://doi.org/10.1145/3551708.3551765},
}

