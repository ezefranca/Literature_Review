@Article{Tuah2022,
  author              = {Tuah, Nooralisa Mohd and Ghani, Siti Khadijah Abd. and Darham, Suryani and Sura, Suaini},
  journal             = {International Journal of Advanced Computer Science and Applications},
  title               = {A Food Waste Mobile Gamified Application Design Model using UX Agile Approach in Malaysia},
