  isbn                = {978-145038643-2},
  journal             = {ACM International Conference Proceeding Series},
  keywords            = {Design; Human engineering; Sustainable development; Emotional reactions; Emotional response; Food literacy; Food waste; Greenhouse emissions; Industrialized countries; Multiple-choice questions; Provocative designs; Real foods; Sustainable consumption; Serious games},
  language            = {English},
  publication_stage   = {Final},
  source              = {Scopus},
  type                = {Conference paper},
  url                 = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85125881579&doi=10.1145%2f3490632.3505171&partnerID=40&md5=07dcb02eff870398ee4fafa874b43cf2},
}

