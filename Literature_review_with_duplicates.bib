  isbn                = {978-989870447-4},
  journal             = {Proceedings of the International Conferences on e-Society 2023, ES 2023 and Mobile Learning 2023, ML 2023},
  keywords            = {Augmented reality; Conservation; E-learning; Multimedia systems; Sustainable development; Education for sustainability; EduPARK app; Game-Based; Game-based Learning; Innovative learning; Local environments; Mobile games; Mobile Learning; Outdoor learning; Sustainability learning; Students},
