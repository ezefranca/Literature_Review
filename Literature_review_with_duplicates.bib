@Article{Hezarkhani2023,
  author                 = {Hezarkhani, Behzad and Demirel, Guven and Bouchery, Yann and Dora, Manoj},
  journal                = {European Journal of Operational Research},
  title                  = {Can “ugly veg” supply chains reduce food loss?},
  year                   = {2023},
  issn                   = {03772217},
  note                   = {Cited by: 9; All Open Access, Hybrid Gold Open Access},
  number                 = {1},
  pages                  = {117 – 132},
  volume                 = {309},
  abbrev_source_title    = {Eur J Oper Res},
  abstract               = {The tradition of marketing only aesthetically agreeable produce by retailers contributes to a major source of food loss through “ugly veg”, i.e., the produce that does not look “regular”. In this paper, we examine the relations between different tiers of agri-food supply chains to study the impact of marketing ugly veg on different supply chain members and the food loss in the system. We examine and compare scenarios of a centralized supply chain, a traditional supply chain without ugly veg, an ugly veg supply chain with a single retailer offering both regular produce and ugly veg, and a two-retailer supply chain where an auxiliary retailer sells the ugly veg. We characterize the equilibrium decisions in these systems and also provide analytical results and insights on the effectiveness of different supply chain designs based on a comprehensive numerical study. We demonstrate the conditions under which the supply chain can reduce overall food loss. For sufficiently high cost of effort, selling ugly veg through the single retailer reduces food loss. Nonetheless, the grower is generally better off offering the ugly veg to an auxiliary retailer. We show that the ratio of food loss per cultivated land always decreases in the two-retailer supply chain, while the total food loss might increase for sufficiently high cost of effort. © 2023 The Author(s)},
  affiliations           = {Brunel Business School, Brunel University, Kingston Lane, London, UB8 3PH, United Kingdom; School of Business and Management, Queen Mary University of London, Mile End Road, London, E1 4NS, United Kingdom; The Centre of Excellence in Supply Chain (CESIT), KEDGE Business School, 680 cours de la Libération, Talence, 33400, France; Faculty of Business and Law, Anglia Ruskin University, East Road, Cambridge, CB1 1PT, United Kingdom},
  author_keywords        = {Agri-food supply chains; Constraint optimization; Food loss; Non-cooperative game theory},
  coden                  = {EJORD},
  correspondence_address = {B. Hezarkhani; Brunel Business School, Brunel University, London, Kingston Lane, UB8 3PH, United Kingdom; email: behzad.hezarkhani@brunel.ac.uk},
  doi                    = {10.1016/j.ejor.2023.01.033},
  keywords               = {Commerce; Food supply; Game theory; Sales; Supply chains; Agri-food supply chains; Analytical results; Centralized supply chains; Condition; Constraint optimizations; Food loss; High costs; Non-cooperative game theory; Retailer supply chains; Supply-chain designs; Constrained optimization},
  language               = {English},
  publication_stage      = {Final},
  publisher              = {Elsevier B.V.},
  source                 = {Scopus},
  type                   = {Article},
  url                    = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85147314973&doi=10.1016%2fj.ejor.2023.01.033&partnerID=40&md5=b90c91fa00f9e718e5e69c1631be30d9},
}

@Article{Sato2022,
