@InProceedings{Song2024,
  author    = {Song, Wen and Kong, Lingyi},
  booktitle = {Proceedings of the 2023 4th International Conference on Computer Science and Management Technology},
  title     = {Risk Assessment for Online Food Delivery Service based on Best-worst Method and Grey Target Decision},
  year      = {2024},
  address   = {New York, NY, USA},
  pages     = {398–403},
  publisher = {Association for Computing Machinery},
  series    = {ICCSMT '23},
  abstract  = {Over the past few years, the safety of online food delivery services has become a matter of significant public concern due to an increase in frequent takeaway-related accidents. To address this issue, this study focuses on the risk assessment for the online food delivery services using the best-worst method and grey target decision method. Firstly, the influencing factors of online food delivery services’ risk are studied from the aspects of online food delivery platforms, restaurants, riders and customers. The risk factor system of online food delivery services is established according to the literature analysis method. Secondly, according to the characteristics of the risk, a risk assessment method combines best-worst method and grey target decision is constructed to measure the risk levels for different participants and risk factors. Finally, based on the risk assessment results, several decision schemes are proposed from the perspectives of participants. The grey target decision method is used to optimize the decision schemes, and after calculation, it is found that formulating the package standards to ensure food safety is the satisfactory decision scheme.},
